6-
find ~/ -type d

7-
sudo find / -type f -name ".profile" 2>>/dev/null

8-
ls -l /etc | grep -w passwd
ls -l /dev/pts | grep -w 0
ls -l /dev | grep -w sda
ls -l / | grep -w etc

9-
sudo ls -d -i /etc
sudo ls -d -i /etc/hosts
sudo ls -d -i /

10-
